{
  "publisher": "Qiskit",
  "name": "qiskit-vscode",
  "version": "0.15.0",
  "license": "Apache-2.0",
  "displayName": "Qiskit Code Assistant (Beta)",
  "description": "Write and optimize Qiskit code with a generative AI code assistant powered by IBM watsonx",
  "author": "Qiskit",
  "icon": "/assets/qiskit-vscode.png",
  "galleryBanner": {
    "color": "#100f11",
    "theme": "dark"
  },
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Qiskit/qiskit-code-assistant-vscode.git"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "python",
    "qiskit",
    "keybindings",
    "snippet"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run clear-out && npm run build-base",
    "build-base": "esbuild ./src/extension.ts --bundle --outfile=./out/extension.js --external:vscode --format=cjs --platform=node",
    "build": "npm run build-base -- --sourcemap",
    "build-watch": "npm run build -- --watch",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests",
    "test": "vscode-test",
    "vsce:package": "vsce package --no-rewrite-relative-links",
    "clear-out": "rimraf ./out"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/chai-as-promised": "^8.0.2",
    "@types/debounce": "^1.2.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.12.7",
    "@types/rimraf": "^4.0.5",
    "@types/semver": "^7.3.4",
    "@types/sinon": "^17.0.4",
    "@types/tmp": "^0.2.0",
    "@types/vscode": "^1.88.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^2.25.0",
    "chai": "^6.2.0",
    "chai-as-promised": "^8.0.2",
    "esbuild": "^0.25.8",
    "glob": "^11.0.3",
    "mocha": "^11.7.4",
    "rimraf": "^5.0.5",
    "sinon": "^21.0.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "debounce": "^2.0.0",
    "semver": "^7.3.2",
    "tmp": "^0.2.1",
    "vsce": "^2.15.0"
  },
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "when": "editorLangId == python",
          "command": "qiskit-vscode.migrate-code",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "qiskit-vscode.api-token",
        "title": "Qiskit Code Assistant: Set IBM Quantum API token"
      },
      {
        "command": "qiskit-vscode.select-credential",
        "title": "Qiskit Code Assistant: Select Credential",
        "enablement": "qiskit-vscode.api-token-set"
      },
      {
        "command": "qiskit-vscode.reset-credential-selection",
        "title": "Qiskit Code Assistant: Reset Credential Selection"
      },
      {
        "command": "qiskit-vscode.migrate-code",
        "title": "Qiskit Code Assistant: Migrate code",
        "icon": "$(sparkle)",
        "enablement": "qiskit-vscode.api-token-set"
      }
    ],
    "configuration": {
      "title": "Qiskit Code Assistant",
      "properties": {
        "qiskitCodeAssistant.url": {
          "type": "string",
          "default": "https://qiskit-code-assistant.quantum.ibm.com",
          "format": "url",
          "description": "URL to the Qiskit Code Assistant Service API",
          "tags": [
            "usesOnlineServices"
          ]
        },
        "qiskitCodeAssistant.selectedCredential": {
          "type": "string",
          "default": "",
          "markdownDescription": "The selected credential name from qiskit-ibm.json to use for authentication. Leave empty to use automatic selection based on priority order. Use the 'Qiskit Code Assistant: Select Credential' command to choose from available credentials."
        },
        "qiskitCodeAssistant.enableTelemetry": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Controls whether telemetry is collected. Setting `telemetry.telemetryLevel` to `off` will take precedence.",
          "tags": [
            "telemetry",
            "usesOnlineServices"
          ]
        },
        "qiskitCodeAssistant.enableStreaming": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable streaming of completions data for real-time code generation",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.streamingBufferSize": {
          "type": "number",
          "default": 1,
          "minimum": 1,
          "maximum": 10,
          "markdownDescription": "Number of chunks to buffer before yielding (higher values may improve performance but increase latency)",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.streamingRetryAttempts": {
          "type": "number",
          "default": 2,
          "minimum": 0,
          "maximum": 5,
          "markdownDescription": "Number of retry attempts for failed streaming requests",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.streamingRetryDelay": {
          "type": "number",
          "default": 1000,
          "minimum": 100,
          "maximum": 5000,
          "markdownDescription": "Base delay in milliseconds between retry attempts (uses exponential backoff)",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.streamingTimeout": {
          "type": "number",
          "default": 30000,
          "minimum": 5000,
          "maximum": 120000,
          "markdownDescription": "Timeout in milliseconds for streaming requests (0 = no timeout)",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.streamingDebug": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "Enable detailed debug logging for streaming operations (shows chunk counts, bytes, parse errors)",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.circuitBreakerEnabled": {
          "type": "boolean",
          "default": true,
          "markdownDescription": "Enable circuit breaker to temporarily disable streaming after repeated failures",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.circuitBreakerThreshold": {
          "type": "number",
          "default": 3,
          "minimum": 1,
          "maximum": 10,
          "markdownDescription": "Number of consecutive failures before circuit breaker opens",
          "tags": [
            "streaming"
          ]
        },
        "qiskitCodeAssistant.circuitBreakerResetTimeout": {
          "type": "number",
          "default": 60000,
          "minimum": 10000,
          "maximum": 300000,
          "markdownDescription": "Time in milliseconds to wait before attempting recovery after circuit opens",
          "tags": [
            "streaming"
          ]
        }
      }
    },
    "keybindings": [
      {
        "key": "tab",
        "command": "qiskit-vscode.accept-suggestion",
        "when": "inlineSuggestionVisible"
      },
      {
        "key": "escape",
        "command": "qiskit-vscode.dismiss-suggestion",
        "when": "inlineSuggestionVisible"
      },
      {
        "key": "ctrl+z",
        "mac": "cmd+z",
        "command": "qiskit-vscode.escape-inline-suggestion",
        "when": "qiskit-vscode.snippet-suggestion:enabled && qiskit-vscode.in-inline-suggestions || qiskit-vscode.inline-suggestion:enabled && qiskit-vscode.in-inline-suggestions"
      },
      {
        "key": "alt+]",
        "command": "qiskit-vscode.next-inline-suggestion",
        "when": "qiskit-vscode.snippet-suggestion:enabled && qiskit-vscode.in-inline-suggestions || qiskit-vscode.inline-suggestion:enabled && qiskit-vscode.in-inline-suggestions"
      },
      {
        "key": "alt+[",
        "command": "qiskit-vscode.prev-inline-suggestion",
        "when": "qiskit-vscode.snippet-suggestion:enabled && qiskit-vscode.in-inline-suggestions || qiskit-vscode.inline-suggestion:enabled && qiskit-vscode.in-inline-suggestions"
      },
      {
        "key": "ctrl+.",
        "command": "qiskit-vscode.handle-get-completion"
      }
    ]
  }
}
